    "import requests\n",
    "from bs4 import BeautifulSoup as bs\n",
    "url = 'http://www.hncb.com.tw/ibankqa/ebanking_trans_q1.shtml'\n",
    "res = requests.get(url)\n",
    "res.encoding = 'big5'\n",
    "soup = bs(res.text)\n",
    "soup1 = soup.select('.listbg') \n",
    "soup2 = soup1[0].text.strip()\n",
    "x='\\n'.join(soup2.split())\n",
    "print x\n",
    "    \n"
   ]
  }
